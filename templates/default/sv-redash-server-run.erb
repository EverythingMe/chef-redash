#!/bin/sh
cd <%= @options[:current_path] %>
exec 2>&1
. <%= @options[:env_path] %>
exec chpst -u <%= @options[:user] %> env <%= ::File.join(@options[:virtualenv_path], 'bin', 'gunicorn') %> -b 127.0.0.1:<%= @options[:web_port] %> --name <%= @options[:name] %> -w <%= @options[:web_workers] %> redash.wsgi:app

